{
  "$schema": "../../../SchemaGenerator/Schema/EdityProject.json",
  "contentMap": {
    "bootstrap": {
      "css": [ "~/lib/bootstrap/dist/css/bootstrap-custom.css" ]
    },
    "transitions": {
      "css": [ "~/lib/edity/css/fade.css" ]
    },
    "edityLoading": {
      "css": [ "~/lib/edity/css/loading.css" ]
    },
    "diff_match_patch": {
      "js": [ { "file": "~/edity/lib/diff_match_patch/diff_match_patch.js" } ]
    },
    "codemirror": {
      "js": [
        { "file": "~/edity/lib/codemirror/lib/codemirror.js" },
        { "file": "~/edity/lib/codemirror/mode/xml/xml.js" },
        { "file": "~/edity/lib/codemirror/mode/css/css.js" },
        { "file": "~/edity/lib/codemirror/mode/javascript/javascript.js" },
        { "file": "~/edity/lib/codemirror/mode/htmlmixed/htmlmixed.js" },
        { "file": "~/edity/lib/codemirror/addon/merge/merge.js" }
      ],
      "css": [
        "~/edity/lib/codemirror/lib/codemirror.css",
        "~/edity/lib/codemirror/theme/edity.css",
        "~/edity/lib/codemirror/addon/merge/merge.css"
      ],
      "dependencies": [ "diff_match_patch" ]
    },
    "ckeditor": {
      "js": [ { "file": "~/edity/lib/ckeditor/ckeditor.js" } ]
    },
    "editorCore": {
      "dependencies": [ "transitions", "edityLoading", "codemirror" ]
    }
  },
  "vars": {
    "uploadUrl": "~/edity/upload",
    "pageSettingsUrl": "~/edity/page/settings",
    "deletePageUrl": "~/edity/Page",
    "mediaBrowseDir": "~/",
    "pageTemplateUrl": "~/edity/templates",
    "editToolsBackground": ""
  },
  "additionalContent": [ "lib/bootstrap/dist/fonts" ],
  "compilers": [
    {
      "type": "Html",
      "template": "default.html",
      "extension": ".html"
    }
  ]
}